{
    "name": "zabaala/laravel_precommit",
    "description": "A git pre-commit hook to Laravel apps.",
    "type": "library",
    "require": {
        "php": ">=7.0.0",
        "phpunit/phpunit": "~6.0",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mauricio Rodrigues",
            "email": "mauricio.vsr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zabaala\\PreCommit\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zabaala\\PreCommit\\PreCommitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
